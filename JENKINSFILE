    NEW_VERSION='10.1'
    
    
    }
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Build') { 
            steps { 
                echo "in buid" 
                sleep 3
                echo "build version "  ${NEW_VERSION}
            }
        }
        stage('Test'){
            steps {
                echo "in  tessssssssst" 
                sleep 5
            }
        }
        stage('Deploy') {
            steps {
                echo "in DDDDDDDDDDDDDDDDDeply" 
            }
        }
    }
    
    
}
