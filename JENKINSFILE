pipeline { 
    agent any 
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Build') { 
            steps { 
                echo "in buid" 
                sleep 3
            }
        }
        stage('Test'){
            steps {
                echo "in  tessssssssst" 
                sleep 5
            }
        }
        stage('Deploy') {
            steps {
                echo "in DDDDDDDDDDDDDDDDDeply" 
            }
        }
    }
    
    post{
         always{}
         failure{}
         success{}
        }
}
