pipeline{
    agent{
        label 'agentx'
    }
     tools {
        maven 'maven-old' 
    }
    stages{
        stage('checkout'){
            steps{
                checkout scm
            }
        }
        stage('build'){
            steps{
                sh "mvn clean install"
            }
        }
        stage('maven-version'){
            steps{
                sh "mvn --version"
            }
        }
    }

}
