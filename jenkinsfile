pipeline{
    agent{
        label 'agentx'
    }
     tools {
        maven 'maven-new' 
    }
    stages{
        stage('checkout'){
            steps{
                checkout scm
            }
        }
        stage('build'){
            steps{
                sh "mvn clean install -DskipTests"
            }
        }
        stage('maven-version'){
            steps{
                sh "mvn --version"
            }
        }
        stage('Test'){
            steps{
             echo "hello from the feature branch"
            sh "mvn test"
            junit allowEmptyResults: true, testResults: 'target/surefire-reports/*xml'
            }
    }

}
