pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Merge') {
            steps {
              sh 'git checkout master'
              sh 'git merge origin/dev'
              sh 'git push master'
            }
        }
    }
}
