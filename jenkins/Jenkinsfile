pipeline{
    agent any
    stages{
        stage("Build"){
            steps{
                echo 'This is from my feature branch'
		sh'mvn -B -DskipTests clean package'
            }
        }
        stage("Test"){
            steps{
	        echo 'This is from my feature branch' 
                sh'mvn test'
            }
            post{
                always{
                    junit'target/surefire-reports/*.xml'
                }
            }
        }
        stage("Deliver"){
            steps{
                echo 'This is from my feature branch'
		sh'./jenkins/scripts/deliver.sh'
            }
        }
    }
}
