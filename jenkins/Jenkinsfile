pipeline {
    agent any
    stages {
                stage('Build') {
                    steps {
                        bat 'mvn -B -DskipTests clean package'
                    }
                }
                stage('Test') {
                            steps {
                                bat 'mvn test'
                            }
                            post {
                                always {
                                    junit 'target/surefire-reports/*.xml'
                                }
                            }
                }
        stage ('complete'){
            steps {
                echo 'Hurrah I configured the poll scm'
            }
        }
    }
}
